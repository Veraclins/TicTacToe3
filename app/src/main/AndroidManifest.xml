<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Select5x5Board" />
        <activity android:name=".Select3x3Board" />
        <activity android:name=".SelectBoard" />
        <activity android:name=".PlaySinglePlayerGame" />
        <activity android:name=".PlayTwoPlayerGame" />
        <activity android:name=".SelectMarker" />
    </application>

</manifest>